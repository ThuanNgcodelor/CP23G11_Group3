<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="thuan.dev.controller.ShowFeedback">
   <children>
      <AnchorPane prefHeight="612.0" prefWidth="718.0">
         <children>
