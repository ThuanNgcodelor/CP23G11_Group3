                  <TableColumn fx:id="table_newsID" prefWidth="52.0" text="ID" />
                  <TableColumn fx:id="table_newsName" prefWidth="502.0" text="FeedBack" />
                  <TableColumn fx:id="table_newsDetails" prefWidth="127.0" text="date" />
