            </TableView>
            <Button fx:id="close" layoutX="671.0" layoutY="14.0" mnemonicParsing="false" onAction="#close" prefHeight="26.0" prefWidth="33.0" styleClass="btn" text="X" />

            <TextField fx:id="searchField" layoutX="417.0" layoutY="14.0" prefHeight="35.0" prefWidth="224.0" promptText="Search" stylesheets="@../resource/Admin.css" />
